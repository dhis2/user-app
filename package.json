{
  "name": "user-app",
  "version": "31.0.0",
  "description": "DHIS2 User app",
  "private": true,
  "license": "BSD-3-Clause",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:style": "d2-style js check --all",
    "prestart": "npm run localize",
    "prebuild": "npm run localize",
    "pretest": "npm run localize",
    "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
    "localize": "npm run extract-pot && d2-i18n-generate -n user-app -p ./i18n/ -o ./src/locales/"
  },
  "dependencies": {
    "@dhis2/d2-i18n": "^1.0.3",
    "@dhis2/d2-ui-sharing-dialog": "^5.3.0",
    "@dhis2/ui": "^1.0.0-beta.14",
    "d2": "31.0.1",
    "d2-ui": "29.0.25",
    "d2-utilizr": "^0.2.15",
    "d3-color": "^1.0.3",
    "material-design-icons-iconfont": "^4.0.5",
    "material-ui": "^0.20.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-highlight-words": "^0.11.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "3.0.1",
    "redux": "^3.7.2",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.5.6",
    "trashable": "^1.0.6",
    "typeface-roboto": "^0.0.54"
  },
  "devDependencies": {
    "@dhis2/cli-style": "^3.2.2",
    "@dhis2/d2-i18n-extract": "^1.0.6",
    "@dhis2/d2-i18n-generate": "^1.0.18",
    "husky": "^2.3.0",
    "redux-logger": "^3.0.6"
  },
  "husky": {
    "hooks": {
      "commit-msg": "d2-style commit check",
      "pre-commit": "d2-style js apply"
    }
  }
}
